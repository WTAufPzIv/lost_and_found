/* 第二版界面 */
.listItemBox{
  width: 100%;
  background-color: white;
  display: flex;
  flex-direction: column;
  padding-bottom: 10px;
  padding-left: 10px;
  padding-right: 10px;
  margin-top: 10px;
}
.listItemHead{
  width: 100%;
  height: 85px;
  /* background-color: green; */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.listItemAvatar{
  width: 60px;
  height: 60px;
  border-radius: 100px;
  overflow:hidden;
}
.listItemHeadRight{
  height: 80px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 10px;
}
.listItemHeadLeft{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.listItemNickname{
  height: 40px;
  line-height: 40px;
  font-size: 20px;
  font-weight: bold;
}
.listItemTime{
  font-size: 12px;
  height: 25px;
  color:rgb(160,160,160)
}
.moreSet{
  height: 70px;
  width: 70px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.listItemText{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.listItemTextOne{
  font-size: 17px;
  color:#FFD84D;
}
.listItemTextTwo{
  font-size: 17px;
  margin-left: 5px;
}
.listItemImgBox{
  margin-top:15px;
  width: 75%;
  padding-bottom: 0px;
}
.listItemImg{
  border-radius: 10px;
  width: 100%;
}
.listItemAddress{
  margin-top:10px;
  display: flex;
  flex-direction: row;
  justify-content: start;
  align-items: center;
  font-size: 15px;
  color:rgb(150,150,150);
}
.addressImg{
  width: 15px;
  height: 15px;
}





.classListItem{
  height: 18px;
  /* width: 30px; */
  border-color: #FFD84D;
  border-style: solid;
  border-width: 1px;
  color:rgb(50,50,50);
  font-size: 12px;
  line-height: 18px;
  padding:6px;
  border-radius: 6px;
  margin:10px 10px;
}
.addButton{
 position: absolute;
  width:50px;
  height: 50px;
  border-radius: 100px;
  background-color: #FFD84D;
  right: 20px;
  top:75%;
  opacity: 0.6;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.searchHis{
  position: absolute;
  width: 80%;
  margin:0px auto;
  padding-bottom: 40px;
  padding-top: 5px;
  background-color: white;
  z-index: 100000;
  padding-left: 20px;
  flex-direction: column;
  padding-right: 20px;
  box-shadow: 1px 1px 2px rgb(180,180,180);
  border-radius: 2px;
}
.searchHisItem{
  height: 18px;
  padding: 4px;
  font-size: 12px;
  color:rgb(120,120,120);
  background-color: rgb(240,240,240);
  border-radius: 3px;
  margin:5px 5px;
}