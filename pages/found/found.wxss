/**index.wxss**/
page{
  height: 100%;
  width: 100%;
  margin:0;
  padding:0;
}
.nav{
  width: 100%;
  height: 75px;
  background-color: #FFD84D;
  text-align: center;
  line-height: 95px;
  position: fixed;
}
.navText{
  color: black;
  font-size: 20px;
}
.noChooseTotal{
  width:100%;
  height:89.7%;
  background-color:rgb(240,240,240);
  position:relative;
  top:0px;
  overflow: hidden;
}
.noChooseImg{
  width: 100px;
  height: 100px;
}
.needloginImg{
  width: 300px;
  height: 300px;
}
.noChooseImgBox{
  width: 100px;
  height: 100px;
  margin:150px auto;
  margin-bottom: 10px;
}
.needloginImgBox{
  width: 300px;
  height: 300px;
  margin:50px auto;
  margin-bottom: 10px;
}
.noChooseTextBox{
  text-align: center;
}
.noChooseTextBox{
  font-size: 15px;
  color:rgb(120,120,120);
}

.noChooseButton{
  width: 250px;
  height: 40px;
  border-radius: 5px;
  border-color: rgb(120,120,120);
  color:rgb(120,120,120);
  border-width: 1px;
  border-style: solid;
  text-align: center;
  line-height: 40px;
  margin:30px auto;
}
.total{
  width:100%;
  height:89.7%;
  background-color:rgb(240,240,240);
  position:relative;
  top:75px;
  overflow: hidden;
}
.top{
  position: relative;
  top:0px;
  width: 100%;
  height: 40px;
  background-color:#FFD84D;
}
.searchBox{
  width: 80%;
  height: 32px;
  background-color: rgba(255,255,255,0.5);
  border-radius: 200px;
  margin:0 auto;
  color: rgb(150,150,150);
  font-size: 15px;
  line-height: 32px;
  padding-left: 50px;
  background-image: url(data:image/png;base64,);
  background-size: 25px 25px;
  background-repeat: no-repeat;
  background-position-x: 15px;
  background-position-y: 5px;
}
.disText{
  width: 90%;
  padding-left: 20px;
  padding-right: 20px;
  height: 50px;
  /* background-color: blue; */
}
.classNameBox{
  float:right;
  padding:5px;
  font-size: 12px;
  height: 18px;
  border-width: 2px;
  border-color: #FFD84D;
  border-style: solid;
  border-radius: 6px;
  position: relative;
  top:8px;
  line-height: 18px;
  color:white;
  background-color: #FFD84D;
  text-align: center;
  box-shadow: 1px 1px 2px rgb(180,180,180);
}
.classNameList{
  /* padding-left: 30px;
  padding-right: 30px; */
  position: absolute;
  width: 100%;
  background-color:rgb(240,240,240);
  z-index: 10000;
  transition: 0.2s;
  flex-direction: row;
  flex-wrap:wrap;
  justify-content: flex-start;
  align-content:flex-start;
}
.hr{
  position: relative;
  top:4px;
  width: 100%;
  height: 1px;
  background-color: rgb(220,220,220);
}
.list{
  width: 100%;
  background-color: rgb(240,240,240);
}
.empty{
  width: 100%;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  /* background-color: blue; */
}
.emptyText{
  font-size: 17px;
  height: 20px;
  margin:10px auto;
  color:rgb(160,160,160);
}
::-webkit-scrollbar {
       width: 0;
       height: 0;
       color: transparent;
    }
.listHeadText{
  position: relative;
  margin-top: 5px;
  font-size: 12px;
  color:rgb(120,120,120);
  width:100%;
  height: 20px;
  line-height: 20px;
}



