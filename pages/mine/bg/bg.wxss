page{
  height: 100%;
  width: 100%;
  margin:0;
  padding:0;
}
.nav{
  width: 100%;
  height: 75px;
  background-color: #FFD84D;
  text-align: center;
  line-height: 95px;
  position: fixed;
  z-index: 10000;
}
.navText{
  color: black;
  font-size: 20px;
}
.navImg{
  width: 30px;
  height: 30px;
  position: absolute;
  left:15px;
}
.body{
  position: relative;
  display: flex;
  flex-direction: column;
  justify-items: center;
  align-items: center;
  padding-bottom: 30px;
}
.imgItem{
  width: 95%;
  height: 220px;
  border-radius: 10px;
  position: relative;
  margin-top:10px;
}