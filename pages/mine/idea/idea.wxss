page{
  height: 100%;
  width: 100%;
  margin:0;
  padding:0;
}
.nav{
  width: 100%;
  height: 75px;
  background-color: #FFD84D;
  text-align: center;
  line-height: 95px;
  position: fixed;
}
.navText{
  color: black;
  font-size: 20px;
}
.navImg{
  width: 30px;
  height: 30px;
  position: absolute;
  left:15px;
}
.body{
  position: relative;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content:flex-start;
  align-content: center;
  align-items: center;
  background-color: rgb(250,250,250);
}
.text{
  width: 94%;
  height: 200px;
  font-size: 18px;
  padding-left: 10px;
  padding-right: 10px;
  background-color: white;
  padding-top: 20px;
}
.con{
  position: relative;
  top:5px;
  width: 94%;
  height: 40px;
  font-size: 18px;
  padding-left: 10px;
  padding-right: 10px;
 background-color: white;
}
.btn{
  width: 60%;
  height: 40px;
  border-radius: 100px;
  background-color: #FFD84D;
  color:white;
  text-align: center;
  line-height: 40px;
  position: relative;
  top:30px;
}