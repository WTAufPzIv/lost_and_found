page{
  height: 100%;
  width: 100%;
  margin:0;
  padding:0;
}
.nav{
  width: 100%;
  height: 75px;
  background-color: white;
  text-align: center;
  line-height: 95px;
  position: fixed;
  z-index: 1;
}
.navText{
  color: black;
  font-size: 20px;
}
.navImg{
  width: 30px;
  height: 30px;
  position: absolute;
  left:15px;
}
.body{
  width: 100%;
  display: flex;
  flex-direction: column;
}
.img{
  width: 95%;
  height: 300px;
}
.imgdetail{
  width: 95%;
  margin:10px auto;
}
.head{
  position: relative;
  top:30px;
  padding-left: 15px;
}
.headItem{
  font-size: 18px;
  height: 35px;
}
.describe{
  font-size: 18px;
  color:rgb(140,140,140);
  position: relative;
  top:24px;
  text-indent:35px;
  padding: 15px;
}
.describe>p{
  text-indent:35px
}
.person{
  position: relative;
  top:60px;
  width: 100%;
  display: flex;
  /* s */
  /* justify-content: center; */
  flex-direction: column;
  justify-content: center;
  align-content: center;
  height: 150px;
}
.avatarBox{
  width: 50px;
  height: 50px;
  border-radius: 100px;
  overflow: hidden;
  /* display: flex;
  justify-content: center; */
  align-self:center;
}
.avatar{
  width: 50px;
  height: 50px;
}
.button{
  width: 100px;
  height: 30px;
  color:white;
  background-color: #FFD84D;
  border-radius: 5px;
  font-size: 15px;
  align-self:center;
  margin:5px auto;
  text-align: center;
  line-height: 30px;
}
.name{
  align-self:center;
  font-size: 15px;
  color:rgb(140,140,140);
  margin:5px auto;
}
.carousel{
  height: 200px;
  width:100%;
  overflow: hidden;
}
.carousel image{
  width:100%;
}